{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/sendmail", "methods": ["POST"], "dest": "api/index.py" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
